#include <iostream>
using namespace std;

int main() {
    int n = 7;              
    int arr[n] = {4, 9, 1, 7, 12, 3, 6};

    int max = arr[0];

    for (int i = 1; i < n; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
    }

    cout << "Maximum number: " << max;

    return 0;
}
