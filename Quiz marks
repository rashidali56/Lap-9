#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter number of students: ";
    cin >> n;

    int marks[n];
    cout << "Enter marks: ";
    for (int i = 0; i < n; i++) {
        cin >> marks[i];
    }

    int count = 0;
    for (int i = 0; i < n; i++) {
        if (marks[i] >= 50) {
            count++;
        }
    }

    cout << "Number of passed students: " << count;

    return 0;
}
